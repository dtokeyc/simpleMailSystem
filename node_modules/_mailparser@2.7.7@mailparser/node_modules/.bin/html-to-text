#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_html-to-text@5.1.1@html-to-text/bin/cli.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../_html-to-text@5.1.1@html-to-text/bin/cli.js" "$@"
  ret=$?
fi
exit $ret
